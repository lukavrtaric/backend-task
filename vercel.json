{
    "version": 2,
    "builds": [
        {
            "src": "/(.*)",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "PUT",
                "DELETE",
                "GET",
                "POST"
            ]
        }
    ],
    "buildCommand": "nest build"
}